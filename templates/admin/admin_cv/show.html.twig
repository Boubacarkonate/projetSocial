{% extends 'base.html.twig' %}

{% block title %}Cv{% endblock %}

{% block body %}
    <h1>Cv</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cv.id }}</td>
            </tr>
            <tr>
                <th>Job</th>
                <td>{{ cv.job }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ cv.createdAt ? cv.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ cv.updatedAt ? cv.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_cv_index') }}">back to list</a>

    <a href="{{ path('app_admin_cv_edit', {'id': cv.id}) }}">edit</a>

    {{ include('admin/admin_cv/_delete_form.html.twig') }}
{% endblock %}
